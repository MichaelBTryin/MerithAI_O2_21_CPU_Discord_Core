# Discord Voice Bot Environment Variables
# Copy this file to .env and fill in your values

# Discord Bot Token (get from Discord Developer Portal)
# https://discord.com/developers/applications
DISCORD_TOKEN=your_bot_token_here

# Optional: LM Studio API settings (if not using localhost:1234)
# LM_STUDIO_URL=http://192.168.1.100:1234/v1
# LM_STUDIO_API_KEY=optional_api_key

# Optional: Logging level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Optional: Enable debug mode for troubleshooting
DEBUG=false
